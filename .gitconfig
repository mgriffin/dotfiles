  ui = auto
  pager = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[alias]
  ba = branch -a
  st = status
  ci = commit
  co = checkout
  lg = log --graph --pretty=oneline --abbrev-commit --decorate
  lp = log -p
  sh = show --stat
  s = status -s
